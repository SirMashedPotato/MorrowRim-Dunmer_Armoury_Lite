			<texPath>Things/Pawn/Humanlike/Apparel/MorrowRim_Bonemold/Bonemold_Dres_GahKogo_Bleached/Bonemold_Dres_GahKogo_Bleached_Helmet_Item</texPath>
		</graphicData>
		<costList>
			<Dye>20</Dye>
			<Cloth>10</Cloth>
		</costList>
		<apparel>
			<wornGraphicPath>Things/Pawn/Humanlike/Apparel/MorrowRim_Bonemold/Bonemold_Dres_GahKogo_Bleached/Bonemold_Dres_GahKogo_Bleached_Helmet</wornGraphicPath>
